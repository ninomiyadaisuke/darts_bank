<div class="col-md-offset-5 col-md-4 users-new-container">
  <div class="shop_search_form">
    <%= form_for @shop,url: search_result_shops_path, method: :get do |f| %>
    <div class="field">
      <%= f.label :ショップ都道府県検索 %><br>
      <%= f.select :prefecture_name, Shop.prefectures.keys, {prompt: '選択してください'} %>
      <%= f.submit "検索" %>
    </div>
    <% end %>
  </div>
</div>
