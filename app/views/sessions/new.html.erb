<div class="session-new-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-md-offset-4 col-md-4 users-new-container">
        <h1 class="text-center ">Log in</h1>
          <%= form_for :session, url: login_path do |f| %>
          <div class="form-group">
            <%= f.label :email %>
            <%= f.text_field :email, placeholder: 'Email', class: "form-control" %>
          </div>
          <div class="form-group">
            <%= f.label :password %>
            <%= f.password_field :password, placeholder: 'password入力', class: "form-control" %>
          </div>
          <%= f.submit "ログイン", class:'btn btn-block btn-black' %>
        <% end %>
      </div>
    </div>
  </div>
</div>
